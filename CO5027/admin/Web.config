<?xml version="1.0"?>
<!-- Changes based on Albrecht, (2013) -->
<configuration>
    <system.web>
      <httpRuntime maxRequestLength="51200" executionTimeout="3600" />
    </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="52428800" />
      </requestFiltering>
    </security>
  </system.webServer>
</configuration>
